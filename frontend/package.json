    "@angular/platform-browser-dynamic": "^21.1.5",
    "@angular/router": "^21.1.5",
    "@angular/ssr": "^21.1.4",
    "rxjs": "~7.8.2",
    "tslib": "^2.8.1",
    "winbox": "^0.2.82",
    "zone.js": "~0.15.1"
  },
